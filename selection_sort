def selection_sort(arrayy,len):
    for i in range(len):
        for j in range(i+1,len):
            if a[i] > a[j]:
                a[i] , a[j] = a[j] , a[i]
    return arrayy


a = []
n = int(input("How many number you awnt to add an array :"))
for i in range(0,n):
    num = int(input("Enter Number :"))
    a.append(num)

print(selection_sort(a,n))
